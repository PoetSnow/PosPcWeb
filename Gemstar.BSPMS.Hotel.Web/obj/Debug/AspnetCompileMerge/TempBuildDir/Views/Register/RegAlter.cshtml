
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>捷信达软件注册信息维护</title>
    <style type="text/css">
        <!--
        .title {
            font-size: 16pt;
            color: #00CC00;
        }

        .message {
            font-size: 12pt;
            color: #FF0000;
        }
        -->
    </style>
</head>
<body>
    <div> 
        <table width="100%">
            <tr>
                <td style="width: 3px"></td>
            </tr>
            <tr>
                <td style="width: 3px; height: 17px"></td>
            </tr>
            <tr>
                <td align="center">
                    <table align="center" width="400" cellpadding="0" cellspacing="0">
                        <tr>
                            <td align="left" class="title" colspan="5">
                                重要提示：
                            </td>
                        </tr>
                        <tr>
                            <td colspan="5">
                                &nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td class="message" colspan="5" align="left">
                                &nbsp;&nbsp;&nbsp;&nbsp;<span id="errorMessage" style="display:inline-block;width:400px;">@ViewData["errorMessage"]</span>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="5">
                                &nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td colspan="5">
                                &nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td colspan="5">
                                <a href="@Url.Action("Index","Account")"> 确定 </a>&nbsp;&nbsp;
                            </td>
                        </tr>
                        <tr>
                            <td colspan="5"></td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
