@model Gemstar.BSPMS.Hotel.Services.EntitiesPosProcedures.up_pos_list_billByBillidResult

@{
    if (Model != null)
    {
        <ul id="PosBill">
            <li>客账号：<span>@Model.BillNo</span></li>
            <li>开台卡号：<span>@(Model.Invno)</span></li>
            <li>营业点：<span>@Model.refeName</span></li>
