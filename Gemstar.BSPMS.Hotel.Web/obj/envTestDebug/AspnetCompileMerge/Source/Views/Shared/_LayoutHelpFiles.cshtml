
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title 帮助中心</title>
    @Html.Partial("_HeaderCss")
    @Html.Partial("_HeaderJs")

    <style type="text/css">
        .message-hd {
            font-size: 18px;
            font-weight: bold;
            color: #fff;
            padding-left: 18.5%;
            line-height: 60px;
            background: #222222;
            letter-spacing: 2px;
        }
    </style>
    @RenderSection("headerCss",false)
    @RenderSection("headerJs",false)
</head>
<body>
    <div class="message-hd">帮助中心<span style="display:inline-block;float:right;margin-right:20px;">@string.Format("{0}  程序时间：{1}",MvcApplication.IsTestEnv? "测试版本":"正式版本",ViewBag.createTime)</span></div>
    @RenderBody()
    @RenderSection("scripts",false)
</body>
</html>
