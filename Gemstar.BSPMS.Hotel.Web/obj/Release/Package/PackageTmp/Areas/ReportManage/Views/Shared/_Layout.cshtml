<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - @Html.Partial("_SystemNameInTitle")</title>
    @Html.Partial("_HeaderJs")
    @RenderSection("headerContent", false)
</head>
<body>
    @Html.Partial("_HeaderCss")
    <style type="text/css">body {line-height: normal;}.pace-done body {line-height: normal;}</style>
    <div id="loadingDiv"></div>
    <div class="wrap clearfix">
        @RenderBody()
    </div>
    @Html.Partial("_CommonQueryWindow")
    @RenderSection("scripts", false)
</body>
</html>