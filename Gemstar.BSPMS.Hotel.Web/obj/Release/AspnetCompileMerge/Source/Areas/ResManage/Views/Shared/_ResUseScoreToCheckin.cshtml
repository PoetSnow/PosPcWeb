@*积分换房*@
<div id="orderDetailUseScoreToCheckinWindow">
    <div class="k-grid k-widget" style="max-height: 476px;overflow:hidden;">
        <div class="k-grid-header" style="padding-right: 17px;">
            <div class="k-grid-header-wrap k-auto-scrollable">
                <table>
                    <colgroup>
                        <col style="width:196px;">
                        <col>
                    </colgroup>
                    <thead>
                        <tr>
                            <th class="k-header">日期</th>
                            <th class="k-header">执行积分</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
        <div class="k-grid-content k-auto-scrollable" style="max-height: 357px;">
            <table style="height: auto;">
                <colgroup>
                    <col style="width:196px;">
                    <col>
                </colgroup>
                <tbody id="orderDetailUseScoreToCheckinWindow_Tbody"></tbody>
            </table>
        </div>
        <div class="k-header k-grid-toolbar">
            <div id="orderDetailUseScoreToCheckinWindow_editPriceDiv" style="text-align:center;">
                <button class="k-button" id="orderDetailUseScoreToCheckinWindow_submit">确定</button>
                <button class="k-button" id="orderDetailUseScoreToCheckinWindow_cancel">取消</button>
                <input type="hidden" id="orderDetailUseScoreToCheckinWindow_SuccessCallBackFunction" />
            </div>
            <div id="orderDetailUseScoreToCheckinWindow_editPriceDivMsg" class="toolbar" style="padding-right: 9.6px;text-align: center;display:none;">积分不足</div>
        </div>
    </div>
</div>